{
    "containerDefinitions": [
        {
            "name": "flask-app",
            "image": "kordaraworkshops/flask-app:dev",
            "essential": true
        }
    ],
    "family": "flask-app",
    "executionRoleArn": "<ECS Task Execution Role ARN here>",
    "networkMode": "awsvpc",
    "requiresCompatibilities": [
        "EC2",
        "FARGATE"
    ],
    "cpu": "256",
    "memory": "512",
    "runtimePlatform": {
        "cpuArchitecture": "X86_64",
        "operatingSystemFamily": "LINUX"
    }
}